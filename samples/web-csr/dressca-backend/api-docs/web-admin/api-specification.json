            "format": "int64"
          },
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "type": "integer",
            "format": "int64"
          },
          "productCode": {
            "type": "string"
          },
          "rowVersion": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "catalogBrandId",
          "catalogCategoryId",
          "description",
          "name",
          "price",
          "productCode"
        ]
      },
      "PutCatalogItemRequest": {
        "type": "object",
        "properties": {
          "catalogBrandId": {
            "type": "integer",
            "format": "int64"
          },
          "catalogCategoryId": {
            "type": "integer",
            "format": "int64"
          },
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "type": "integer",
            "format": "int64"
          },
          "productCode": {
            "type": "string"
          }
        },
        "required": [
          "catalogBrandId",
          "catalogCategoryId",
          "description",
          "name",
          "price",
          "productCode"
        ]
      },
      "UserResponse": {
        "type": "object",
        "properties": {
          "role": {
            "type": "string"
          },
          "userName": {
            "type": "string"
