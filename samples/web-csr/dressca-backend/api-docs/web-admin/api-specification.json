              "application/vnd.spring-boot.actuator.v2+json": {
                "schema": {
                  "type": "object"
                }
              },
              "application/vnd.spring-boot.actuator.v3+json": {
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "OK"
          }
        },
        "summary": "Actuator web endpoint \u0027health\u0027",
        "tags": [
          "Actuator"
        ]
      }
    },
    "/api/health/**": {
      "get": {
        "operationId": "health-path",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              },
              "application/vnd.spring-boot.actuator.v2+json": {
                "schema": {
                  "type": "object"
                }
              },
              "application/vnd.spring-boot.actuator.v3+json": {
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "OK"
          }
        },
        "summary": "Actuator web endpoint \u0027health-path\u0027",
        "tags": [
          "Actuator"
        ]
      }
    },
    "/api/users": {
      "get": {
        "description": "ユーザーの情報。",
        "operationId": "getLoginUser",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponse"
                }
              }
            },
            "description": "成功."
          },
          "401": {
            "description": "未認証エラー."
          }
        },
        "summary": "ログイン中のユーザーの情報を取得します。",
        "tags": [
          "Users"
        ]
      }
    }
  },
  "components": {
